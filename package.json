{
  "name": "@light-town/api",
  "version": "0.5.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:watch:cov": "npm run test:watch -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.config.e2e.js --runInBand",
    "test:e2e:watch": "npm run test:e2e -- --watch --coverage",
    "db:seeds:run": "npm run build && node dist/src/db/seeds/index.js",
    "test:prod": "jest --runInBand --coverage --config ./jest.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/light-town/api.git"
  },
  "author": "MichailShcherbakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/light-town/api/issues"
  },
  "homepage": "https://github.com/light-town/api#readme",
